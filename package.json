{
  "name": "obsidian-sonar",
  "version": "0.0.0",
  "description": "Deep semantic search for your Obsidian notes, completely offline",
  "main": "main.js",
  "type": "module",
  "scripts": {
    "build:worker": "node scripts/esbuild-worker.mjs production",
    "build:inline": "node scripts/emit-worker-inline.mjs",
    "build": "npm run build:worker && npm run build:inline && tsc -noEmit -skipLibCheck && node esbuild.config.mjs production",
    "clean": "rm main.js && rm -rf src/generated",
    "dev": "node esbuild.config.mjs",
    "deploy": "./deploy.sh",
    "version": "node version-bump.mjs && git add manifest.json versions.json",
    "sonar": "npx tsx scripts/sonar.ts",
    "sonar:index": "npx tsx scripts/sonar.ts index",
    "sonar:search": "npx tsx scripts/sonar.ts search",
    "sonar:stats": "npx tsx scripts/sonar.ts stats",
    "sonar:config": "npx tsx scripts/sonar.ts config",
    "parse-line-chat": "npx tsx scripts/parse-line-chat.ts",
    "fix:format": "prettier --write .",
    "fix:lint": "eslint . --fix",
    "fix": "npm run fix:format && npm run fix:lint",
    "check:format": "prettier --check .",
    "check:lint": "eslint . --max-warnings 0",
    "check:svelte": "svelte-check --tsconfig tsconfig.json",
    "check:tsc": "npx tsc --noEmit",
    "check": "npm run check:format && npm run check:lint && npm run check:svelte && npm run check:tsc"
  },
  "keywords": [
    "obsidian",
    "obsidian-plugin",
    "semantic-search",
    "embeddings",
    "bm25",
    "transformers",
    "webgpu",
    "llamacpp",
    "local",
    "offline"
  ],
  "author": "aviatesk",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/aviatesk/obsidian-sonar"
  },
  "dependencies": {
    "@huggingface/transformers": "^3.7.6",
    "commander": "^12.0.0",
    "d3": "^7.9.0",
    "lucide": "^0.544.0"
  },
  "devDependencies": {
    "@eslint/js": "^9.36.0",
    "@types/d3": "^7.4.3",
    "@types/node": "^24.3.0",
    "@typescript-eslint/eslint-plugin": "^8.41.0",
    "@typescript-eslint/parser": "^8.41.0",
    "@webgpu/types": "^0.1.66",
    "builtin-modules": "^5.0.0",
    "esbuild-svelte": "^0.9.3",
    "eslint-plugin-svelte": "^3.12.4",
    "eslint-plugin-unused-imports": "^4.2.0",
    "husky": "^9.1.7",
    "lint-staged": "^16.1.6",
    "obsidian": "^1.8.7",
    "prettier": "^3.6.2",
    "svelte": "^5.39.4",
    "svelte-check": "^4.3.2",
    "svelte-eslint-parser": "^1.3.3",
    "svelte-preprocess": "^6.0.3",
    "tslib": "^2.8.1",
    "tsx": "^4.20.5",
    "typescript": "^5.9.2"
  }
}
