{
  "name": "obsidian-sonar",
  "version": "0.1.4",
  "description": "Deep knowledge retrieval for Obsidian, completely offline",
  "main": "main.js",
  "type": "module",
  "engines": {
    "node": ">=24.0.0"
  },
  "scripts": {
    "build": "tsc -noEmit -skipLibCheck && node esbuild.config.mjs production",
    "build:with-benchmark": "tsc -noEmit -skipLibCheck && INCLUDE_BENCHMARK=true node esbuild.config.mjs production",
    "clean": "rm main.js",
    "dev": "node esbuild.config.mjs",
    "dev:with-benchmark": "INCLUDE_BENCHMARK=true node esbuild.config.mjs",
    "deploy": "./deploy.sh",
    "version": "node version-bump.mjs && git add manifest.json versions.json",
    "sonar": "npx tsx scripts/sonar.ts",
    "sonar:index": "npx tsx scripts/sonar.ts index",
    "sonar:search": "npx tsx scripts/sonar.ts search",
    "sonar:stats": "npx tsx scripts/sonar.ts stats",
    "sonar:config": "npx tsx scripts/sonar.ts config",
    "parse-line-chat": "npx tsx scripts/parse-line-chat.ts",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:ui": "vitest --ui",
    "fix:format": "prettier --write .",
    "fix:lint": "eslint . --fix",
    "fix": "npm run fix:format && npm run fix:lint",
    "check:format": "prettier --check .",
    "check:lint": "eslint . --max-warnings 0",
    "check:svelte": "svelte-check --tsconfig tsconfig.json",
    "check:tsc": "npx tsc --noEmit",
    "check": "npm run check:format && npm run check:lint && npm run check:svelte && npm run check:tsc"
  },
  "keywords": [
    "obsidian",
    "obsidian-plugin",
    "semantic-search",
    "embeddings",
    "bm25",
    "llamacpp",
    "local",
    "offline"
  ],
  "author": "aviatesk",
  "license": "AGPL-3.0-or-later",
  "repository": {
    "type": "git",
    "url": "https://github.com/aviatesk/obsidian-sonar"
  },
  "dependencies": {
    "commander": "^14.0.3",
    "d3": "^7.9.0",
    "zod": "^4.3.6"
  },
  "devDependencies": {
    "@eslint/js": "^9.36.0",
    "@types/d3": "^7.4.3",
    "@types/node": "^25.2.0",
    "@typescript-eslint/eslint-plugin": "^8.41.0",
    "@typescript-eslint/parser": "^8.41.0",
    "@vitest/ui": "^4.0.18",
    "@webgpu/types": "^0.1.66",
    "builtin-modules": "^5.0.0",
    "esbuild-svelte": "^0.9.3",
    "eslint-plugin-svelte": "^3.12.4",
    "eslint-plugin-unused-imports": "^4.2.0",
    "husky": "^9.1.7",
    "lint-staged": "^16.2.7",
    "obsidian": "^1.11.0",
    "pdfjs-dist": "^5.4.449",
    "prettier": "^3.6.2",
    "svelte": "^5.39.4",
    "svelte-check": "^4.3.2",
    "svelte-eslint-parser": "^1.3.3",
    "svelte-preprocess": "^6.0.3",
    "tslib": "^2.8.1",
    "tsx": "^4.20.5",
    "typescript": "^5.9.2",
    "vitest": "^4.0.18"
  }
}
